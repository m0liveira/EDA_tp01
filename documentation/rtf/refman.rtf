00445 {\cf18 void} getShortestPath(Graph* graph, Edge* edge, {\cf18 int} startVertex, {\cf18 int} numVertices) \{\par
00446     {\cf18 int} distances[MAX_VERTICES];\par
00447     {\cf18 int} prev[MAX_VERTICES];\par
00448     {\cf18 int} visited[MAX_VERTICES];\par
00449 \par
00450     {\cf20 // Initialize distances and previous vertices}\par
00451     {\cf19 for} ({\cf18 int} i = 0; i < numVertices; i++) \{\par
00452         distances[i] = INT_MAX;\par
00453         prev[i] = -1;\par
00454         visited[i] = 0;\par
00455     \}\par
00456 \par
00457     {\cf20 // Set distance of start vertex to 0}\par
00458     distances[startVertex] = 0;\par
00459 \par
00460     {\cf20 // Find shortest path for all vertices}\par
00461     {\cf19 for} ({\cf18 int} i = 0; i < numVertices - 1; i++) \{\par
00462         {\cf20 // Find the vertex with the minimum distance}\par
00463         {\cf18 int} minDistance = INT_MAX;\par
00464         {\cf18 int} minVertex = -1;\par
00465 \par
00466         {\cf19 for} ({\cf18 int} j = 0; j < numVertices; j++) \{\par
00467             {\cf19 if} (!visited[j] && distances[j] < minDistance) \{\par
00468                 minDistance = distances[j];\par
00469                 minVertex = j;\par
00470             \}\par
00471         \}\par
00472 \par
00473         {\cf19 if} (minVertex == -1) \{\par
00474             {\cf19 break};\par
00475         \}\par
00476 \par
00477         {\cf20 // Mark the minimum distance vertex as visited}\par
00478         visited[minVertex] = 1;\par
00479 \par
00480         {\cf20 // Update distances of adjacent vertices}\par
00481         Edge* currentEdge = edge;  {\cf20 // Use a separate pointer to traverse the edges}\par
00482 \par
00483         {\cf19 while} (currentEdge != NULL) \{\par
00484             {\cf18 int} neighbor = currentEdge->vertexB;\par
00485             {\cf18 float} distance = currentEdge->distance;\par
00486 \par
00487             {\cf19 if} (!visited[neighbor] && distances[minVertex] != INT_MAX && distances[minVertex] + distance < distances[neighbor]) \{\par
00488                 distances[neighbor] = distances[minVertex] + distance;\par
00489                 prev[neighbor] = minVertex;\par
00490             \}\par
00491 \par
00492             currentEdge = currentEdge->nextEntry;\par
00493         \}\par
00494     \}\par
00495 \par
00496     printf({\cf22 "\\n\\n"});\par
00497     {\cf19 for} ({\cf18 int} i = 2; i < numVertices; i++) \{\par
00498         printf({\cf22 "Rota mais rapida: "});\par
00499         printPath(prev, i);\par
00500         printf({\cf22 " (%.2fkm)\\n"}, distances[i]);\par
00501     \}\par
00502     printf({\cf22 "\\n\\n"});\par
00503 \}\par
