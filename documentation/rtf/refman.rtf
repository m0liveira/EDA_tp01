00340     {\cf19 while} (startEntry != NULL)\{\par
00341         {\cf19 if} (startEntry->id == {\cf18 id}) {\cf19 return} 1;\par
00342 \par
00343         startEntry = startEntry->nextEntry;\par
00344     \}\par
00345 \par
00346     {\cf19 return} 0;\par
00347 \}\par
00348 \par
00358 Vehicle *rechargeVehicles(Vehicle *startEntry)\{\par
00359     Vehicle *currentEntry = startEntry;\par
00360 \par
00361     {\cf19 if} (currentEntry == NULL) \{\par
00362         printf({\cf22 "\\nNao ha veiculos registados!\\n\\n"});\par
00363         {\cf19 return} NULL;\par
00364     \}\par
00365 \par
00366     {\cf19 while} (currentEntry != NULL) \{\par
00367         currentEntry->currentBattery = 100;\par
00368         currentEntry = currentEntry->nextEntry;\par
00369     \}\par
00370     \par
00371     printf({\cf22 "Todos os veiculos foram recarregados!\\n\\n"});\par
00372     {\cf19 return} startEntry;\par
00373 \}\par
